            "metadata_startup_script": "#!/bin/bash\nset -euxo pipefail\n\nmkdir -p /var/log/autodeploy\nexec \u003e \u003e(tee -a /var/log/autodeploy/startup.log) 2\u003e\u00261\n\n# Base tools\napt-get update -y\nDEBIAN_FRONTEND=noninteractive apt-get install -y docker.io git curl ca-certificates python3 python3-venv python3-pip\n\nsystemctl enable --now docker || true\n\nIMAGE=\"docker.io/cockckd/hello_world:latest\"\nPLATFORM=\"\"\nARCH_OK=0\nPULL_OK=0\n\n# Write env file\ninstall -d -m 0755 /opt\ncat \u003e /opt/app.env \u003c\u003c'EOF'\nPORT=5000\nHOST=0.0.0.0\nEOF\n\nif [ -n \"$IMAGE\" ]; then\n  # Check if amd64 exists\n  if docker manifest inspect \"$IMAGE\" \u003e/dev/null 2\u003e\u00261; then\n    if docker manifest inspect \"$IMAGE\" | grep -q '\"architecture\": \"amd64\"'; then\n      ARCH_OK=1\n    fi\n  fi\n\n  if [ \"$ARCH_OK\" -eq 1 ]; then\n    if docker pull \"$IMAGE\"; then\n      PULL_OK=1\n    fi\n  else\n    # Try arm64 under emulation\n    docker run --privileged --rm tonistiigi/binfmt --install arm64 || true\n    PLATFORM=\"--platform linux/arm64\"\n    if docker pull $PLATFORM \"$IMAGE\"; then\n      PULL_OK=1\n    fi\n  fi\nfi\n\nif [ \"$PULL_OK\" -eq 1 ]; then\n  echo \"[info] Running pulled image: $IMAGE\"\n  docker rm -f autodeploy-app || true\n  docker run -d --name autodeploy-app $PLATFORM --env-file /opt/app.env -p 80:5000 \"$IMAGE\"\n  exit 0\nfi\n\necho \"[warn] Could not pull image or no image provided. Falling back to building from source.\"\n\n# Clone repo and attempt docker build\nrm -rf /opt/app-src\ngit clone --depth=1 \"https://github.com/Arvo-AI/hello_world\" /opt/app-src\n\nif [ -f /opt/app-src/Dockerfile ]; then\n  echo \"[info] Dockerfile found. Building local image...\"\n  docker build -t autodeploy-local:latest /opt/app-src\n  docker rm -f autodeploy-app || true\n  docker run -d --name autodeploy-app --env-file /opt/app.env -p 80:5000 autodeploy-local:latest\n  exit 0\nfi\n\necho \"[warn] No Dockerfile in repo. Running natively via systemd.\"\n\n# Minimal Node install if requested later\ninstall_node() {\n  if command -v node \u003e/dev/null 2\u003e\u00261; then return 0; fi\n  curl -fsSL https://deb.nodesource.com/setup_18.x | bash -\n  DEBIAN_FRONTEND=noninteractive apt-get install -y nodejs\n}\n\n# Native Python\nif echo \"python\" | grep -Eiq '^python'; then\n  cd /opt/app-src\n  python3 -m venv /opt/app-venv\n  . /opt/app-venv/bin/activate\n  if [ -f requirements.txt ]; then pip install -r requirements.txt || true; fi\n  pip install gunicorn || true\n  cat \u003e/etc/systemd/system/autodeploy.service \u003c\u003cSERVICE\n[Unit]\nDescription=Autodeploy App (Python)\nAfter=network.target\n\n[Service]\nEnvironmentFile=/opt/app.env\nWorkingDirectory=/opt/app-src\nExecStart=/bin/bash -lc 'cd app'\nRestart=always\nUser=root\n\n[Install]\nWantedBy=multi-user.target\nSERVICE\n  systemctl daemon-reload\n  systemctl enable --now autodeploy.service\n  exit 0\nfi\n\n# Native Node\nif echo \"python\" | grep -Eiq '^node'; then\n  install_node\n  cd /opt/app-src\n  if [ -f package.json ]; then npm ci || npm install; fi\n  cat \u003e/etc/systemd/system/autodeploy.service \u003c\u003cSERVICE\n[Unit]\nDescription=Autodeploy App (Node)\nAfter=network.target\n\n[Service]\nEnvironmentFile=/opt/app.env\nWorkingDirectory=/opt/app-src\nExecStart=/bin/bash -lc 'cd app'\nRestart=always\nUser=root\n\n[Install]\nWantedBy=multi-user.target\nSERVICE\n  systemctl daemon-reload\n  systemctl enable --now autodeploy.service\n  exit 0\nfi\n\necho \"[error] Unknown stack; sources are in /opt/app-src. Please install runtime and start manually.\"\nexit 1\n",
